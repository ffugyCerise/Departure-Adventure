rite(\"Megenta.png\")\nelif self.number == 2:\n    self.sprite = sprite(\"Dark.png\")\nelif self.number == 3:\n    self.sprite = sprite(\"Dino.png\")\nelif self.number == 4:\n    self.sprite = sprite(\"Winter.png\")\n","loop":"#Egg loop\nself.x += self.vx\nself.y += self.vy\nself.vy = self.vy - game.gravity\n\n\n\n"}}]},{"Empty":[{"one":{"type":"object script","start":"#one start\n\n","loop":"#one loop\n\n"}},{"C1":{"type":"object script","start":"#C1 start\n\n","loop":"#C1 loop\n\n"}},{"C2":{"type":"object script","start":"#C2 start\n\n","loop":"#C2 loop\n\n"}},{"C3":{"type":"object script","start":"#C3 start\n\n","loop":"#C3 loop\n\n"}},{"C4":{"type":"object script","start":"#C4 start\n\n","loop":"#C4 loop\n\n"}},{"TheEnd":{"type":"object script","start":"#TheEnd start\n\n","loop":"#TheEnd loop\n\n"}}]},{"Advance":{"type":"object script","start":"#Advance start\nself.sprite = sprite(\"Advance.png\")\nself.scaleX= 0.7\nself.scaleY= 0.7\nself.x = 500\nself.y = -300\n\n","loop":"#Advance loop\n\n"}}],"room":[{"intros":[{"Start":{"type":"room script","start":"#Start start\nself.bg = one()\nself.bg.sprite = new_sprite(\"PressToStart.png\")\n\nself.bg.scaleX = 2.5\nself.bg.scaleY = 2.5\n\nself.playA = one()\nself.mobA = one()\n\nsprite_sheet_right = sprite(\"PlayerEast.png\", 1, 8)\nright = new_animation(sprite_sheet_right, 7, 0, 7)\nset_animation(self.playA, right)\n\nself.playA.scaleX = 3\nself.playA.scaleY= 3\nself.playA.x = 0\nself.playA.y = -260\n\nsprite_sheet = sprite(\"Mob.png\", 1, 10)\nidel = new_animation(sprite_sheet, 9, 0, 9)\nset_animation(self.mobA, idel)\n\nself.mobA.scaleX = 4\nself.mobA.scaleY = 4\nself.mobA.x = -550\nself.mobA.y = -180","loop":"#Start loop\nself.playA.x += 6\nif self.playA.x >= 600:\n    self.playA.x = -self.playA.x\n\n\nself.mobA.x += 6\nif self.mobA.x >= 600:\n    self.mobA.x = -self.mobA.x\n\n\nif key_was_pressed(\"enter\"):\n    room_set(\"Ins\")\n\n"}},{"Ins":{"type":"room script","start":"#Ins start\nself.bg = C1()\nself.bg.sprite = new_sprite(\"Ins.png\")\n\nself.bg.scaleX = 2\nself.bg.scaleY = 2\n\nAdvance()","loop":"#Ins loop\nif key_was_pressed(\"enter\"):\n    room_set(\"CutScene1\")\n"}},{"Lore":{"type":"room script","start":"#Lore start\nself.bg = C1()\nself.bg.sprite = new_sprite(\"Lor.png\")\n\nself.bg.scaleX = 1.7\nself.bg.scaleY = 1.7\n\nAdvance()","loop":"#Lore loop\nif key_was_pressed(\"enter\"):\n    room_set(\"Play\")\n\n"}}]},{"CutScenes":[{"CutScene1":{"type":"room script","start":"#CutScene1 start\nself.bg = C1()\nself.bg.sprite = new_sprite(\"CutScene1.png\")\n\nself.bg.scaleX = 2\nself.bg.scaleY = 2\nAdvance()","loop":"#CutScene1 loop\nif key_was_pressed(\"enter\"):\n    room_set(\"CutScene2\")\n"}},{"CutScene2":{"type":"room script","start":"#CutScene2 start\nself.bg = C2()\nself.bg.sprite = new_sprite(\"CutScene2.png\")\n\nself.bg.scaleX = 2\nself.bg.scaleY = 2\nAdvance()","loop":"#CutScene2 loop\nif key_was_pressed(\"enter\"):\n    room_set(\"Cutscene3\")\n\n"}},{"Cutscene3":{"type":"room script","start":"#Cutscene3 start\nself.bg = C3()\nself.bg.sprite = new_sprite(\"CutScene3.png\")\n\nself.bg.scaleX = 2\nself.bg.scaleY = 2\nAdvance()\n","loop":"#Cutscene3 loop\nif key_was_pressed(\"enter\"):\n    room_set(\"Cutscene4\")\n\n"}},{"Cutscene4":{"type":"room script","start":"#Cutscene4 start\nself.bg = C2()\nself.bg.sprite = new_sprite(\"BackStory.png\")\n\nself.bg.scaleX = 2\nself.bg.scaleY = 2\nAdvance()\n","loop":"#Cutscene4 loop\nif key_was_pressed(\"enter\"):\n    room_set(\"Lore\")\n\n\n"}}]},{"Play":{"type":"room script","start":"#self.bGN = new_sound(\"BG.mp3\")\nstop_sound(game.epic) \nloop_sound(game.bGN)\n#this plays the sound. this will play the sound as soon as the game starts\ngame.timer = 0\nBC()\nBC2()\nGround()\nGround2()\nself.dot = CollisionBox()\nself.player = Player()\nmob = Mob()\n#Weapon()\n\n#egg\ngame.gravity = 1\n\neggCreator = Spawner()\neggCreator.x = 800\n\nself.message = new_text(\"Time: \" + str(round(game.timer, 2)), -600, 300)\nself.mes